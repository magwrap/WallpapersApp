{
  "expo": {
    "name": "wallpapers-app",
    "slug": "wallpapers-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallpaper_small.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wallpaper.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wallpaper.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/images/wallpaper.png"
    },
    "description": "An app where you can choose, download, add to favorites wallpapers from pexels API.",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow WallpaperApp to access your photos.",
          "savePhotosPermission": "Allow WallpaperApp to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "githubUrl": "https://github.com/magwrap/WallpapersApp"
  }
}
